{
  "manifest_version": 2,
  "name": "Root Spider",
  "version": "1.0",

  "description": "Searches Webpages for keywords.",


  "browser_action": {
    "default_title": "Root Spider",
    "default_icon": {
      "48": "icons/icon-48.png"
    },
    "default_popup": "popup/search.html"
  },

  "permissions": [
    "activeTab"
  ]
}
